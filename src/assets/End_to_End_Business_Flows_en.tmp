    <h1 id="name">{{data.title}}</h1>

    {{#if data.parent}}
    <h4 id="parent">Parent : <a class="parentlink elementlink" data-id="{{data.parent}}" data-type="{{data.table}}" >{{data.parent}}</a></h4>
    {{/if}}

    {{#oc data}}{{/oc}}
    <h3>Overview</h3>
    <p id="description">{{breaklines data.overview}}</p>
    <h3>Functionality</h3>
    <p id="functionality">{{breaklines data.functionality}}</p>
    <h3>Relation Data</h3>
    <p id="relationData">
    {{#each data.relationData}}
        <b>{{@key}}</b><br />
        <table>
            <thead>
                <tr>
                    <th>ID</th><th>名称</th>
                </tr>
            </thead>
            <tbody>
            {{#each this}}
                <tr>
                    <td><a class="parentlink elementlink" data-id="{{this.name}}" data-type="{{this.table}}" >{{this.name}}</a></td>
                    <td>{{this.title}}</td>
                </tr>
            {{/each}}
            </tbody>
        </table>
        <br />
    {{/each}}
    </p>