    <h1 id="name">{{data.title}}</h1>

    {{#if data.parent}}
    <h4 id="parent">Parent : <a id="parentlink" class="parentlink" data-id="{{data.parent}}" data-type="{{data.table}}" >{{data.parent}}</a></h4>
    {{/if}}

    {{#oc data}}{{/oc}}
    <h3>Overview</h3>
    <p id="description">{{data.overview}}</p>
    <h3>Functionality</h3>
    <p id="functionality">{{data.functionality}}</p>
    <h3>Relation Data</h3>
    <p id="relationData">
    {{#each data.relationData}}
        <b>{{@key}}</b><br />
        {{#each this}}
            <a class="parentlink" data-id="{{this.name}}" data-type="{{this.table}}" >{{this.name}}</a>:{{this.title}}<br />
        {{/each}}
        <br />
    {{/each}}
    </p>