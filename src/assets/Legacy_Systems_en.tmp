
    {{#if data.parent}}
    <h1 id="name">{{data.name}} - {{data.title}}</h1>
    <h4 id="category">Category: {{data.category}}</h4>
    <h4 id="identifier">Identifier: {{data.name}}</h4>
    
    <h4 id="parent">Parent Identifier: <a class="parentlink elementlink" data-id="{{data.parent}}" data-type="{{data.table}}" >{{data.parent}}</a></h4>
    {{#oc data}}{{/oc}}
    <h3>Overview</h3>
    <p id="description">{{breaklines data.overview}}</p>
    <h3>Functionality</h3>
    <p id="functionality">{{breaklines data.functionality}}</p>
    <h3>Relation Data</h3>
    <p id="relationData">
    {{#each data.relationData}}
        <b>{{@key}}</b><br />
        <table>
            <thead>
                <tr>
                    <th>ID</th><th>名称</th>
                </tr>
            </thead>
            <tbody>
            {{#each this}}
                <tr>
                    <td><a class="parentlink elementlink" data-id="{{this.name}}" data-type="{{this.table}}" >{{this.name}}</a></td>
                    <td>{{this.title}}{{#ifEquals this.source "consideration"}}
                        <span class="message fa-stack maturity_level_{{this.maturity_level}}" style="font-size: 0.6em;">
                            <i class="fa fa-circle fa-stack-2x "></i>
                            <i class="fa fa-exclamation fa-stack-1x fa-inverse"></i>
                            <span class="remark">source:{{this.source}}  <br />maturity_level:{{this.maturity_level}} <br />{{#if this.memo}}{{this.memo}}{{/if}}</span>
                        </span>
                         {{else}}
                        <span class="message fa-stack maturity_level_{{this.maturity_level}}" style="font-size: 0.6em;" >
                            <i class="fa fa-certificate fa-stack-2x"></i>
                            <i class="fa fa-check fa-stack-1x fa-inverse"></i>
                            <span class="remark">source:{{this.source}}  <br />maturity_level:{{this.maturity_level}} <br />{{#if this.memo}}{{this.memo}}{{/if}}</span>
                        </span>
                        {{/ifEquals}}
                    </td>
                </tr>
            {{/each}}
            </tbody>
        </table>
        <br />
    {{/each}}
    </p>
    {{else}}
    <h1 id="name">System List</h1>
        <table>
            <thead>
                <tr>
                    <th>ID</th><th>Name</th>
                </tr>
            </thead>
            <tbody>
            {{#each data.children}}
                <tr>
                    <td><a class="parentlink elementlink" data-id="{{name}}" data-type="{{table}}" >{{name}}</a></td>
                    <td>{{title}}</td>
                </tr>
            {{/each}}
            </tbody>
        </table>
    {{/if}}
