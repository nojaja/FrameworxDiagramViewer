    <h1 id="name">{{data.name}} - {{data.title}}</h1>
    <h4 id="category">カテゴリー: {{data.category}}</h4>
    <h4 id="identifier">ID: {{data.name}}</h4>
    <h4 id="maturity_level">成熟度: {{data.maturity}}</h4>
    
    {{#if data.parent}}
    <h4 id="parent">上位ID: <a class="parentlink elementlink" data-id="{{data.parent}}" data-type="{{data.table}}" >{{data.parent}}</a></h4>
    {{#oc data}}{{/oc}}
    {{else}}
        {{#each data.children}}
        <a class="parentlink elementlink" data-id="{{name}}" data-type="{{table}}" >{{name}}</a>:{{title}}<br />
        {{/each}}
    {{/if}}

    <h3>概要</h3>
    <p id="description">{{breaklines data.overview}}</p>
    <h3>機能性</h3>
    <p id="functionality">{{breaklines data.functionality}}</p>
    <h3>関連要素</h3>
    <p id="relationData">
    {{#each data.relationData}}
        <b>{{@key}}</b><br />
        <table>
            <thead>
                <tr>
                    <th>ID</th><th>名称</th>
                </tr>
            </thead>
            <tbody>
            {{#each this}}
                <tr>
                    <td><a class="parentlink elementlink" data-id="{{this.name}}" data-type="{{this.table}}" >{{this.name}}</a></td>
                    <td>{{this.title}}</td>
                </tr>
            {{/each}}
            </tbody>
        </table>
        <br />
    {{/each}}
    </p>